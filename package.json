{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --dev --single --no-clear",
    "start:dev": "sirv public --dev --single",
    "serve": "concurrently \"nodemon server.js\" \"npm run dev\" "
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "concurrently": "^6.5.1",
    "express": "^4.17.2",
    "nodemon": "^2.0.15",
    "rollup": "^2.61.1",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0"
  },
  "dependencies": {
    "sirv-cli": "^1.0.0",
    "svelte-bootstrap-icons": "^1.7.2",
    "svelte-navigator": "^3.1.5"
  }
}
